{
  "name": "contacts-monorepo",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=web\" \"npm run dev --workspace=api\"",
    "build": "npm run build --workspace=web && npm run build --workspace=api"
  },
  "devDependencies": {
    "@types/papaparse": "^5.3.16",
    "concurrently": "^9.1.2",
    "knip": "^5.59.0"
  },
  "dependencies": {
    "@radix-ui/react-radio-group": "^1.3.7",
    "@uppy/core": "^4.4.5",
    "@uppy/drag-drop": "^4.1.3",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "react-day-picker": "^9.7.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1"
  }
}