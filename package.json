{
  "name": "dimicall-electron",
  "private": true,
  "version": "2.0.6",
  "description": "Application de gestion de contacts DimiCall",
  "author": "Dimultra",
  "main": "./dist/main/main.js",
  "homepage": "./",
  "type": "module",
  "scripts": {
    "dev": "electron-vite dev",
    "prebuild": "electron-vite build",
    "build": "electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "start": "electron-vite preview",
    "dist": "npm run build && electron-builder",
    "dist:win": "npm run build && electron-builder --win",
    "dist:mac": "npm run build && electron-builder --mac",
    "dist:linux": "npm run build && electron-builder --linux",
    "test:sim": "node scripts/test-sim-selection.js",
    "analyze:xml": "node scripts/test-xml-analysis.cjs"
  },
  "dependencies": {
    "@calcom/embed-react": "^1.5.3",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "2.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "2.1.7",
    "@radix-ui/react-popover": "1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@supabase/supabase-js": "^2.50.0",
    "@tailwindcss/vite": "^4.1.10",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.11",
    "@types/uuid": "^10.0.0",
    "@yume-chan/adb": "^2.1.0",
    "@yume-chan/adb-daemon-webusb": "^2.1.0",
    "appium": "^2.19.0",
    "appium-uiautomator2-driver": "^4.2.4",
    "better-auth": "^1.2.10",
    "better-sqlite3": "^12.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.19.1",
    "lucide-react": "^0.522.0",
    "papaparse": "^5.5.3",
    "react": "^19.1.0",
    "react-day-picker": "^9.7.0",
    "react-dom": "^19.1.0",
    "react-resizable-panels": "^3.0.3",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "4.1.10",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "web-vitals": "^5.0.3",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "devDependencies": {
    "@electron-toolkit/utils": "^4.0.0",
    "@types/node": "^22.14.0",
    "@types/papaparse": "^5.3.16",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "^9.2.0",
    "electron": "36.5.0",
    "electron-builder": "^26.0.12",
    "electron-vite": "^3.1.0",
    "typescript": "~5.8.3",
    "vite": "^6.3.5"
  },
  "overrides": {
    "@radix-ui/react-dismissable-layer": "1.1.8",
    "@radix-ui/react-focus-scope": "1.1.2",
    "@radix-ui/react-portal": "1.1.8"
  },
  "build": {
    "appId": "com.dimultra.dimicall",
    "productName": "DimiCall",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "public/**/*"
    ],
    "asarUnpack": [
      "dist/renderer/**/*"
    ],
    "extraResources": [
      {
        "from": "platform-tools-latest-windows (4)/platform-tools",
        "to": "platform-tools",
        "filter": [
          "**/*"
        ]
      }
    ],
    "mac": {
      "icon": "public/icons/icon-512x512.svg",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ]
    },
    "win": {
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ],
      "forceCodeSigning": false,
      "extraFiles": [
        {
          "from": "node_modules/electron/dist/icudtl.dat",
          "to": "icudtl.dat"
        },
        {
          "from": "node_modules/electron/dist/*.pak",
          "to": "./"
        }
      ]
    },
    "portable": {
      "artifactName": "${productName}-${version}-portable.${ext}",
      "unpackDirName": "DimiCall",
      "requestExecutionLevel": "user"
    },
    "linux": {
      "icon": "public/icons/icon-512x512.svg",
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  }
}
